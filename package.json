{
  "name": "kylieis.online",
  "description": "Kylie Stewart's personal website",
  "version": "4.0.0",
  "author": "Kylie Stewart <kylie@kyliestewart.tech>",
  "bugs": {
    "url": "https://github.com/kale-stew/kale-stew.github.io/issues"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "test-build": "yarn run build && gatsby serve",
    "deploy": "gatsby clean && gatsby build && gh-pages -b master -d public",
    "dev": "gatsby develop",
    "start": "yarn run dev",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*/**.{js,jsx,scss}' 'gatsby-*.js' 'cli/*.js'",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "new-now": "node ./cli/new-now.js"
  },
  "dependencies": {
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-sharp": "^2.5.11",
    "intersection-observer": "^0.5.1",
    "node-sass": "^4.11.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.0",
    "react-scripts": "3.2.0",
    "react-social-icons": "^4.1.0",
    "react-switch": "^5.0.0",
    "react-typography": "^0.16.19",
    "typeface-fira-sans": "^0.0.75",
    "typography": "^0.16.17",
    "typography-theme-github": "^0.16.18"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.1.1",
    "inquirer": "^6.2.2",
    "prettier": "^1.14.2"
  },
  "keywords": [
    "personal site"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kale-stew/kale-stew.github.io.git"
  }
}
